<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.team2.mapper.RohMapper">

	<insert id="signup">
		insert into p2_member values(#{id},#{pw},#{name},#{memberType},#{ip},sysdate,#{enabled},#{etc1},#{etc2})
	</insert>

	<select id="signin" resultType="int">
		select count(*) from p2_member where id=#{id} and pw=#{pw}
	</select>
</mapper>