<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.team2.mapper.KimMapper">
	<update id="updateSymptomCount" parameterType = "com.team2.component.KimDTO">
		UPDATE symptom_count
		SET count = count + 1
		WHERE symptom_name = #{symptom_Name}
	</update>
</mapper>